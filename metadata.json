{
  "name": "choria-nats",
  "version": "0.0.12",
  "author": "choria",
  "summary": "NATS.io module for the Choria Orchestrator",
  "license": "Apache-2.0",
  "source": "https://github.com/choria-io/puppet-nats",
  "project_page": "https://github.com/choria-io/puppet-nats",
  "issues_url": "https://github.com/choria-io/puppet-nats/issues",
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.12.0 < 5.0.0" },
    { "name": "camptocamp/systemd", "version_requirement": ">= 0.3.0 < 1.0.0" }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.9.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
